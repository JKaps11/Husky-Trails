{
  "expo": {
    "name": "Husky Trails",
    "slug": "husky-trails",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/UConnMapApp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.frisbeebeast11org.huskytrails"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/UConnMapApp.png",
        "backgroundColor": "#000E2F"
      },
      "package": "com.frisbeebeast11org.huskytrails"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-asset",
        {
          "assets": "./assets/uconn.mbtiles"
        }
      ],
      "@maplibre/maplibre-react-native",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/UConnMapApp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000E2F"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "523e88ea-d1f4-4f68-8dbd-4a30e2798210"
      }
    },
    "owner": "seniordesigngroup",
    "updates": {
      "url": "https://u.expo.dev/523e88ea-d1f4-4f68-8dbd-4a30e2798210"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
